    "react-apollo": "^2.1.4",
    "react-dom": "^16.0.0",
    "react-flexbox-grid": "^2.0.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-toolbox": "^2.0.0-beta.12",
